@model IEnumerable<Model.DB_Model.Parametros>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Perfil Parametros
            </th>
            
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                Perfil - Parametros @item.ParametrosId
            </td>
            
            <td>
                <a asp-action="Edit" asp-route-id="@item.ParametrosId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ParametrosId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ParametrosId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
